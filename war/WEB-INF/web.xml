<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>controllerClinicaDental.Login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ClinicaDentalServlet</servlet-name>
		<servlet-class>controllerClinicaDental.ClinicaDentalServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ClinicaDentalServlet</servlet-name>
		<url-pattern>/clinicaDentalServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Search_Dni</servlet-name>
		<servlet-class>controllerClinicaDental.Search_Dni</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Search_Dni</servlet-name>
		<url-pattern>/search_Dni</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>Register_Patient</servlet-name>
		<servlet-class>controllerClinicaDental.Register_Patient</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Register_Patient</servlet-name>
		<url-pattern>/register_Patient</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ViewPatient</servlet-name>
		<servlet-class>viewClinicaDental.ViewPatient</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ViewPatient</servlet-name>
		<url-pattern>/viewPatient</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>NuevaConsulta</servlet-name>
		<servlet-class>controllerClinicaDental.NuevaConsulta</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NuevaConsulta</servlet-name>
		<url-pattern>/nuevaConsulta</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>NewCurrent_Illness</servlet-name>
		<servlet-class>controllerClinicaDental.NewCurrent_Illness</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NewCurrent_Illness</servlet-name>
		<url-pattern>/newCurrent_Illness</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
